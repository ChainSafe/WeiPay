__d(function (global, _require, module, exports, _dependencyMap) {
  'use strict';

  module.exports = function enhanceError(error, config, code, request, response) {
    error.config = config;

    if (code) {
      error.code = code;
    }

    error.request = request;
    error.response = response;
    return error;
  };
});